  "scripts": {
    "lint": "standard && ts-standard",
    "lint:fix": "standard --fix && ts-standard --fix",
    "test": "jest -u --verbose --runInBand --detectOpenHandles --forceExit --coverage",
    "check": "dpdm  \"./src/**\" --exit-code circular:1 --tree=false --warning=false && jscpd \"src\" -r html --output ./jscpd/",
    "watch": "webpack  --watch ",
    "build": "cross-env NODE_ENV=production webpack",
    "start": "cross-env WEBPACK_IS_DEV_SERVER=true NODE_ENV=development webpack serve --config webpack.config.babel.js",
    "hook": "npm run lint:fix & npm run check & npm run test",
    "publish": "ipfs-deploy -C dist -d cloudflare -p pinata "
  },
