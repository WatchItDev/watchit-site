  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "importSort": {
    ".ts, .tsx": {
      "style": "module-and-prefix",
      "parser": "typescript"
    },
    ".js, .jsx": {
      "style": "module-and-prefix",
      "parser": "babylon"
    }
  },
  "standard": {
    "ignore": [
      "dist",
      "webpack.config.babel.js",
      "package.json",
      "/src/**/*.tsx",
      "/src/**/*.ts",
      ".gitignore",
      "/coverage/**",
      "/jscpd/**",
      "/webpack/**/*.js",
      "/test/**/*.js",
      "/deploy/*.js",
      "/tasks/*.js",
      "/utils/*.js"
    ],
    "env": [
      "mocha"
    ]
  },
  "ts-standard": {
    "project": "tsconfig.json",
    "ignore": [
      "dist",
      "webpack.config.babel.js",
      "hardhat.config.js",
      "package.json",
      ".gitignore",
      "/src/i18n",
      "/coverage/**",
      "/jscpd/**",
      "/webpack/**/*.js",
      "/test/**/*.js",
      "/deploy/*.js",
      "/tasks/*.js",
      "/utils/*.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run hook"
    }
  }
